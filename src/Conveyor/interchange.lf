target Python

reactor Interchange(param = "default"){
    //Internal variables
    state name = param
    
    //Physical links
    input IN_UP
    input IN_DOWN 
    output OUT_UP
    output OUT_DOWN

    '''The Interchange is a simple reactor that takes two inputs and forwards them
    to the corresponding outputs'''
    IN_UP -> OUT_DOWN after 0msec
    IN_DOWN -> OUT_UP after 0msec
}